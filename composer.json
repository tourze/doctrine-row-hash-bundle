{
  "name": "tourze/doctrine-row-hash-bundle",
  "description": "Doctrine Row Hash Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "ext-hash": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/messenger": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/async-contracts": "0.0.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/doctrine-entity-checker-bundle": "1.0.*",
    "tourze/doctrine-helper": "0.0.*",
    "tourze/symfony-dependency-service-loader": "1.*",
    "tourze/symfony-lock-command-bundle": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/doctrine-resolve-target-entity-bundle": "1.*",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "DoctrineRowHashBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DoctrineRowHashBundle\\Tests\\": "tests"
    }
  }
}