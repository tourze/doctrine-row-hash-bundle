{
  "name": "tourze/doctrine-row-hash-bundle",
  "description": "Doctrine Row Hash Bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "ext-hash": "*",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "symfony/console": "^6.4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/property-access": "^6.4",
    "symfony/messenger": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1",
    "tourze/doctrine-helper": "0.0.*",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-entity-checker-bundle": "~0.0.8",
    "tourze/symfony-lock-command-bundle": "0.0.*",
    "tourze/symfony-async-bundle": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "DoctrineRowHashBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DoctrineRowHashBundle\\Tests\\": "tests"
    }
  }
}